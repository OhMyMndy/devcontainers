{
    "name": "PHP Alpine",
    "dockerComposeFile": [
        "../docker-compose.yml"
    ],
    "service": "php",
    "remoteUser": "vscode",
    "settings": {
        "terminal.integrated.shell.linux": "/bin/ash"
    },
    "workspaceFolder": "/workspace",
    "extensions": [
        "mandy91.vscode-phpstan",
        "felixfbecker.php-pack",
        "felixfbecker.php-intellisense",
        "adriansanguineti.json-parse-validator"
    ]
}